{
  "name": "@relay/sdk-web",
  "version": "0.1.0",
  "description": "Relay Web SDK - In-app feedback, bug reporting, and session replay",
  "main": "./dist/relay.umd.js",
  "module": "./dist/relay.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/relay.esm.js",
      "require": "./dist/relay.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:unit": "vitest run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "rrweb": "^2.0.0-alpha.11"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "jsdom": "^27.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.2.0"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/relay-dev/relay"
  },
  "keywords": [
    "feedback",
    "bug-reporting",
    "session-replay",
    "customer-feedback",
    "rrweb"
  ],
  "license": "MIT"
}
